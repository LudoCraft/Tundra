material fire
{
	technique
	{
		pass
		{
			lighting off
			depth_write off
			scene_blend alpha_blend
			texture_unit
			{
				texture flame_a.dds
				tex_address_mode clamp


			}
			texture_unit 1
			{
                               texture flame.dds
                               scroll_anim 0.1 0.1
                               colour_op_ex subtract src_texture src_current
			}
		}

	}
}
